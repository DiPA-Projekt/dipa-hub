{
  "name": "dipa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build dipa-api-client && ng build --output-hashing none",
    "build:api": "ng build dipa-api-client",
    "build:prod": "ng build --configuration production dipa-api-client && ng build --configuration production --output-hashing none",
    "test": "ng test",
    "test:ci": "ng test --code-coverage --no-watch --no-progress --browsers=ChromeHeadlessCustom",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ci": "npm run prettier:check && npm run eslint:ci && npm run nyc:ci",
    "eslint": "eslint projects/dipa-frontend/{e2e,src}/**/*.{html,js,json,ts}",
    "eslint:ci": "npm run eslint -- --quiet=true -f json -o eslint-report.json || exit 0",
    "prettier": "prettier projects/dipa-frontend/{src,tests}/**",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "mocha": "mocha",
    "mocha:ci": "npm run mocha || exit 0",
    "nyc": "nyc mocha",
    "nyc:ci": "npm run nyc || exit 0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.13",
    "@angular/cdk": "12.2.12",
    "@angular/common": "12.2.13",
    "@angular/compiler": "12.2.13",
    "@angular/core": "12.2.13",
    "@angular/forms": "12.2.13",
    "@angular/material": "12.2.12",
    "@angular/platform-browser": "12.2.13",
    "@angular/platform-browser-dynamic": "12.2.13",
    "@angular/router": "12.2.13",
    "@types/quill": "^1.3.10",
    "angular-oauth2-oidc": "^10.0.3",
    "angular-resize-event": "2.1.0",
    "bootstrap": "5.0.2",
    "d3": "6.5.0",
    "ngx-quill": "^14.3.0",
    "quill": "^1.3.7",
    "quill-delta-to-html": "^0.12.0",
    "rxjs": "6.6.6",
    "tslib": "2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.13",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "^12.5.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "12.2.13",
    "@angular/compiler-cli": "12.2.13",
    "@dipa-projekt/dipa-client": "1.0.0-beta.1",
    "@types/d3": "6.3.0",
    "@types/jasmine": "3.7.7",
    "@types/jasminewd2": "2.0.9",
    "@types/mocha": "8.2.2",
    "@types/node": "14.14.32",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "chai": "4.3.4",
    "codelyzer": "6.0.1",
    "esbuild": "0.8.53",
    "esbuild-register": "2.0.0",
    "eslint": "7.21.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "35.4.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "esm": "3.2.25",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "7.0.0",
    "jsdom": "16.6.0",
    "jsdom-global": "3.0.2",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "8.3.0",
    "mock-local-storage": "1.1.17",
    "ng-packagr": "12.2.5",
    "npm-check-updates": "^11.1.10",
    "nyc": "15.1.0",
    "prettier": "2.3.2",
    "protractor": "7.0.0",
    "puppeteer": "10.0.0",
    "sinon": "9.2.4",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "~4.3.5"
  }
}