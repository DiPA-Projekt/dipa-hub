{
  "name": "dipa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build dipa-api-client && ng build",
    "build:api": "ng build dipa-api-client",
    "build:prod": "ng build --prod dipa-api-client && ng build --prod",
    "test": "ng test",
    "test:ci": "ng test --code-coverage --no-watch --no-progress --browsers=ChromeHeadlessCustom",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ci": "npm run prettier:check && npm run build:api && npm run eslint:ci && npm run nyc:ci && npm run build:prod",
    "eslint": "eslint projects/dipa-frontend/{e2e,src}/**/*.{html,js,json,ts}",
    "eslint:ci": "npm run eslint -- --quiet=true -f json -o eslint-report.json || exit 0",
    "prettier": "prettier projects/dipa-frontend/{src,tests}/**",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "mocha": "mocha",
    "mocha:ci": "npm run mocha || exit 0",
    "nyc": "nyc mocha",
    "nyc:ci": "npm run nyc || exit 0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.1.1",
    "@angular/cdk": "11.1.1",
    "@angular/common": "11.1.1",
    "@angular/compiler": "11.1.1",
    "@angular/core": "11.1.1",
    "@angular/forms": "11.1.1",
    "@angular/material": "11.1.1",
    "@angular/platform-browser": "11.1.1",
    "@angular/platform-browser-dynamic": "11.1.1",
    "@angular/router": "11.1.1",
    "angular-resize-event": "2.0.1",
    "bootstrap": "4.6.0",
    "d3": "6.5.0",
    "rxjs": "6.6.3",
    "tslib": "2.1.0",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1101.2",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@angular/cli": "11.1.2",
    "@angular/compiler-cli": "11.1.1",
    "@types/d3": "6.3.0",
    "@types/jasmine": "3.6.3",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.22",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.2",
    "chai": "4.2.0",
    "codelyzer": "6.0.1",
    "esbuild": "0.8.39",
    "esbuild-register": "2.0.0",
    "eslint": "7.19.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.6.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "esm": "3.2.25",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "8.2.1",
    "mock-local-storage": "1.1.17",
    "ng-packagr": "11.1.2",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "puppeteer": "6.0.0",
    "sinon": "9.2.4",
